{
    "extends": [
        "config:js-app",
        ":dependencyDashboard",
        ":maintainLockFilesWeekly",
        ":prConcurrentLimitNone",
        ":prHourlyLimitNone",
        ":semanticCommitsDisabled"
    ],
    "docker": {
        "enabled": false
    },
    "rust": {
        "enabled": false
    },
    "postUpdateOptions": ["yarnDedupeHighest"],
    "packageRules": [{
        "packageNames": ["ember-cli", "ember-data", "ember-source"],
        "separateMinorPatch": true
    }]
}
